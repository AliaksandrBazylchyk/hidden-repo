{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Type": "PollConsul",
      "Scheme": "http",
      "Host": "consul",
      "Port": 8500,
      "ConfigurationKey": "gateway",
      "PollingInterval": 100
    }
  }
}
